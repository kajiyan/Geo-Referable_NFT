{
  "name": "@geo-nft/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "copy:onnx": "node scripts/copy-onnx-files.js",
    "dev": "pnpm copy:onnx && next dev",
    "dev:https": "local-ssl-proxy --source 3443 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "dev:ssl": "pnpm copy:onnx && concurrently -k \"next dev\" \"wait-on tcp:3000 && pnpm dev:https\"",
    "generate:certs": "bash scripts/generate-certs.sh",
    "build": "pnpm copy:onnx && next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next node_modules/.cache"
  },
  "dependencies": {
    "@apollo/client": "^4.0.2",
    "@aws-sdk/client-s3": "^3.980.0",
    "@deck.gl/layers": "^8.9.36",
    "@deck.gl/mapbox": "^8.9.36",
    "@geo-nft/client": "workspace:*",
    "@next/third-parties": "^16.1.6",
    "@norosi/types": "workspace:*",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@react-three/drei": "^10.7.5",
    "@react-three/fiber": "^9.5.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@resvg/resvg-js": "^2.6.2",
    "@tanstack/react-query": "^5.85.5",
    "@wagmi/core": "^2.19.0",
    "graphql": "^16.11.0",
    "h3-js": "^4.3.0",
    "idb": "^8.0.3",
    "jsonwebtoken": "^9.0.2",
    "locar": "^0.1.8",
    "lodash.debounce": "^4.0.8",
    "maplibre-gl": "^5.7.1",
    "next": "16.1.6",
    "onnxruntime-web": "^1.23.0",
    "paper": "^0.12.18",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-intersection-observer": "^10.0.2",
    "react-map-gl": "^8.0.4",
    "react-redux": "^9.2.0",
    "siwe": "^3.0.0",
    "suncalc": "^1.9.0",
    "swiper": "^12.0.3",
    "three": "^0.180.0",
    "viem": "^2.34.0",
    "wagmi": "^2.16.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@deck.gl/core": "^8.9.36",
    "@eslint/eslintrc": "^3",
    "@loaders.gl/core": "^3.4.15",
    "@luma.gl/core": "^8.5.21",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/suncalc": "^1.9.2",
    "@types/three": "^0.180.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "16.1.6",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "local-ssl-proxy": "^2.0.5",
    "lucide-react": "^0.561.0",
    "playwright": "^1.56.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^9.0.3"
  }
}
